   }\n        const formData ={\n          email: this.email,\n          password: this.password\n        }\n        try{\n          await this.$store.dispatch('login', formData)\n          await this.$router.push('/')\n        }catch (e) {}\n      }\n    }\n  }\n</script>\n\n<style scoped>\n  .btn{\n    width: 100%;\n  }\n</style>\n"],"sourceRoot":"src/views"}]}