EAAA;AAAA,MAAA,QAAA,EAAA;AAAA,KAHA;AAIA,IAAA,SAAA,EAAA;AAAA,MAAA,QAAA,EAAA;AAAA,KAJA;AAKA,IAAA,MAAA,EAAA;AAAA,MAAA,QAAA,EAAA;AAAA,KALA;AAMA,IAAA,KAAA,EAAA;AAAA,MAAA,QAAA,EAAA;AAAA;AANA,GAbA;AAqBA,EAAA,OAAA,EAAA;AACA,IAAA,aADA,2BACA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qBACA,KAAA,CAAA,EAAA,CAAA,QADA;AAAA;AAAA;AAAA;;AAEA,gBAAA,KAAA,CAAA,EAAA,CAAA,MAAA;;AAFA;;AAAA;AAKA,gBAAA,QALA,GAKA;AACA,kBAAA,KAAA,EAAA,KAAA,CAAA,KADA;AAEA,kBAAA,QAAA,EAAA,KAAA,CAAA,QAFA;AAGA,kBAAA,QAAA,EAAA,KAAA,CAAA,QAHA;AAIA,kBAAA,SAAA,EAAA,KAAA,CAAA,SAJA;AAKA,kBAAA,MAAA,EAAA,KAAA,CAAA,MALA;AAMA,kBAAA,KAAA,EAAA,KAAA,CAAA;AANA,iBALA;AAAA;AAAA;AAAA,uBAcA,KAAA,CAAA,MAAA,CAAA,QAAA,CAAA,UAAA,EAAA,QAAA,CAdA;;AAAA;AAAA;AAAA,uBAeA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,GAAA,CAfA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAiBA;AAlBA;AArBA,CAAA","sourcesContent":["<template>\n  <div class=\"card-deck\">\n    <h1>Регистрация</h1>\n    <form class=\"form-horizontal\" action='' method=\"POST\"\n          @submit.prevent=\"submitHandler\">\n        <div class=\"form-group\">\n          <!-- Username -->\n          <label class=\"control-label\"  for=\"text\">ФИО</label>\n            <input type=\"text\" id=\"text\" name=\"text\" placeholder=\"\" class=\"form-control\"\n                   v-model.trim=\"username\">\n        </div>\n\n        <div class=\"control-group\">\n            <!-- DateBirth -->\n            <label class=\"control-label\" for=\"date\">Дата рождения</label>\n            <input type=\"date\" id=\"date\" name=\"date\" placeholder=\"\" class=\"form-control\"\n                   v-model.trim=\"dateBirth\">\n            <p class=\"help-block\"></p>\n        </div>\n\n        <div class=\"form-group\">\n            <!-- Course and group -->\n            <label class=\"mr-sm-2\" for=\"inlineFormCustomSelect\">Курс</label>\n            <select class=\"custom-select mr-sm-2\" id=\"inlineFormCustomSelect\"\n                    v-model.trim=\"course\">\n                <option value=\"1\">1</option>\n                <option value=\"2\">2</option>\n                <option value=\"3\">3</option>\n                <option value=\"3\">4</option>\n            </select>\n            <label class=\"mr-sm-2\" for=\"inlineFormCustomSelect1\">Группа</label>\n            <select class=\"custom-select mr-sm-2\" id=\"inlineFormCustomSelect1\"\n                    v-model.trim=\"group\">\n                <option value=\"13\">1</option>\n                <option value=\"14\">2</option>\n                <option value=\"13\">3</option>\n                <option value=\"14\">4</option>\n                <option value=\"13\">5</option>\n                <option value=\"14\">6</option>\n                <option value=\"14\">7</option>\n                <option value=\"13\">8</option>\n                <option value=\"14\">9</option>\n                <option value=\"13\">10</option>\n                <option value=\"14\">11</option>\n                <option value=\"14\">12</option>\n                <option value=\"13\">13</option>\n                <option value=\"14\">14</option>\n            </select>\n            <small class=\"form-text text-muted\">Введите курс и группу для корректного отображения расписания занятий</small>\n        </div>\n\n        <div class=\"control-group\">\n          <!-- E-mail -->\n          <label class=\"control-label\" for=\"email\">Электронная почта</label>\n            <input type=\"text\" id=\"email\" name=\"email\" placeholder=\"email\" class=\"form-control\"\n                   autocomplete=\"current-password\"\n                   v-model.trim=\"email\">\n            <p class=\"help-block\"></p>\n        </div>\n\n        <div class=\"control-group\">\n          <!-- Password-->\n          <label class=\"control-label\" for=\"password\">Пароль</label>\n            <input type=\"password\" id=\"password\" name=\"password\" placeholder=\"\" class=\"form-control\"\n                   autocomplete=\"current-password\"\n                   v-model.trim=\"password\">\n        </div>\n\n        <div class=\"control-group\">\n          <button class=\"btn btn-success\">Зарегистрироваться</button>\n          <div class=\"text-center p-t-46 p-b-20\">\n            <span class=\"txt2\">\n                или <router-link class=\"router-link\" to=\"/login\">войти</router-link>\n            </span>\n          </div>\n        </div>\n    </form>\n  </div>\n</template>\n<script>\n    import {email, required, minLength} from 'vuelidate/lib/validators'\n    export default {\n        name: 'Registration',\n        metaInfo:{\n            title: 'Регистрация'\n        },\n        data: () => ({\n            email: '',\n            password: '',\n            username: '',\n            dateBirth: '',\n            course: '',\n            group: ''\n        }),\n        validations: {\n            email: {email, required},\n            password: {required, minLength: minLength(6)},\n            username: {required},\n            dateBirth: {required},\n            course: {required},\n            group: {required},\n        },\n        methods: {\n            async submitHandler (){\n                if(this.$v.$invalid){\n                    this.$v.$touch()\n                    return\n                }\n                const formData ={\n                    email: this.email,\n                    password: this.password,\n                    username: this.username,\n                    dateBirth: this.dateBirth,\n                    course: this.course,\n                    group: this.group,\n                }\n                try{\n                    await this.$store.dispatch('register', formData)\n                    await this.$router.push('/')\n                }catch (e) {}\n            }\n        }\n    }\n</script>\n<style scoped>\n  .btn{\n    width: 100%;\n  }\n</style>\n"],"sourceRoot":"src/views"}]}